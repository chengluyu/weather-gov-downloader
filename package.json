{
  "dependencies": {
    "axios": "^0.21.1",
    "mkdirp": "^1.0.4",
    "pino": "^6.2.0",
    "pino-pretty": "^4.0.0",
    "socks-proxy-agent": "^5.0.0",
    "yaml": "^1.8.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.4"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
